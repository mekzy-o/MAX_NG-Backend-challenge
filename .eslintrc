{
    "env": {
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "extends": [
      "airbnb-base",
      "plugin:prettier/recommended",
      "prettier"
    ],
    "plugins": ["prettier"],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaVersion": 2015
    },
    "rules": {
      "prettier/prettier": "error",
      "doubleQuotes": true
    }
  }